{
  "manifest_version": 2,
  "name": "Wykop Player Remover",
  "version": "1.0",
  "description": "Plugin do usuwania jwplayera z wykop.pl",
  "icons": {
    "48": "icons/wypok.jpg"
  },
  "homepage_url": "https://github.com/trembacz/wykop-player-remover",
  "content_scripts": [
    {
      "matches": ["*://*.wykop.pl/*"],
      "js": ["index.js"]
    }
  ]
}